{
  "name": "taro-hooks",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qnxg/taro-hooks"
  },
  "scripts": {
    "init": "rm -rf node_modules && npm install && npm run clean && npm run bootstrap",
    "start": "npm run dev",
    "dev": "father-doc dev",
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean --yes",
    "compile": "lerna run --scope @qnxg/hooks compile",
    "build": "node ./scripts/clean-old-build.js && father build && npm run compile && node ./scripts/build-wind-up.js",
    "test": "father test",
    "cov": "father test --coverage",
    "help": "father help",
    "precommit": "father pre-commit",
    "build:doc": "father-doc build",
    "pub": "lerna publish"
  },
  "devDependencies": {
    "@tarojs/taro": "^2.0.3",
    "@types/jest": "^25.1.2",
    "del": "^5.1.0",
    "father": "^2.29.2",
    "father-doc": "^1.0.0-alpha.19",
    "lerna": "^3.20.2",
    "typescript": "^3.7.5"
  }
}
